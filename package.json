{
  "name": "testing-jest",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "is-ci-cli test:cover test:watch",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack-dev-server --mode=development",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --mode=production",
    "postbuild": "cp ./public/index.html ./dist/index.html",
    "start": "serve --no-clipboard --single --listen 8080 dist",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|css|html|md)\"",
    "lint": "eslint --ignore-path .gitignore .",
    "validate": "npm run lint && npm run build",
    "setup": "npm install && npm run validate"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "@emotion/babel-preset-css-prop": "^10.0.23",
    "@emotion/jest": "^11.10.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^9.3.2",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.1",
    "eslint-config-kentcdodds": "^14.6.1",
    "eslint-import-resolver-jest": "^3.0.2",
    "file-loader": "^5.0.2",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.2.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "serve": "^11.2.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "emotion-theming": "^10.0.19",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
